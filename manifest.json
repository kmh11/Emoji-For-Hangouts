{
  "manifest_version": 2,
  "name": "Emoji For Hangouts",
  "description": "A simple extension to type emoji in Google Hangouts with emoji codes.",
  "version": "1.0",
	"content_scripts": [{
		"matches": ["*://hangouts.google.com/*"],
		"js": ["jquery.min.js", "emoji_codes.js", "emojityper.js"],
		"all_frames": true
	}],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
